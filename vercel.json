{
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": [
          "app/static/**",
          "app/templates/**",
          "app/data.py"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "app/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "run.py"
    }
  ]
}
